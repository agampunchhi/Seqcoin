ansaction.js","original":{"line":53,"column":34}},{"generated":{"line":116,"column":0},"source":"components/ConductTransaction.js","original":{"line":51,"column":20}},{"generated":{"line":116,"column":18},"source":"components/ConductTransaction.js","original":{"line":50,"column":16}},{"generated":{"line":116,"column":19},"source":"components/ConductTransaction.js","original":{"line":31,"column":12}},{"generated":{"line":116,"column":20},"source":"components/ConductTransaction.js","original":{"line":30,"column":8}},{"generated":{"line":117,"column":0},"source":"components/ConductTransaction.js","original":{"line":58,"column":5}},{"name":"Component","generated":{"line":121,"column":2},"source":"components/ConductTransaction.js","original":{"line":6,"column":33}},{"name":"ConductTransaction","generated":{"line":123,"column":15},"source":"components/ConductTransaction.js","original":{"line":61,"column":15}}],"sources":{"components/ConductTransaction.js":"import React, { Component } from 'react';\nimport { FormGroup, FormControl, Button } from 'react-bootstrap';\nimport { Link } from 'react-router-dom';\nimport history from '../history';\n\nclass ConductTransaction extends Component {\n    state = { recipient: '', amount: 0 };\n    updateRecipient = event => {\n        this.setState({recipient: event.target.value});\n    }\n    updateAmount = event => {\n        this.setState({ amount: Number(event.target.value) });\n    }\n\n    conductTransaction = () => {\n        const { recipient, amount } = this.state;\n\n        fetch(`${document.location.origin}/api/transact`, {\n            method: 'POST',\n            headers: { 'Content-Type': 'application/json'},\n            body: JSON.stringify({ recipient, amount })\n        }).then(response => response.json())\n        .then(json => {\n            alert(json.message || json.type);\n            history.push('/transaction-pool');\n        });\n    }\n\n    render() {\n        return (\n            <div className = 'ConductTransaction'>\n                <Link to = '/'>Home</Link>\n                <h3>CONDUCT A TRANSACTION</h3>\n                <FormGroup>\n                    <FormControl \n                    input = 'text'\n                    placeholder='recipient'\n                    value = {this.state.recipient}\n                    onChange = {this.updateRecipient}\n                    />\n                </FormGroup>\n                <FormGroup>\n                    <FormControl \n                    input = 'number'\n                    placeholder='amount'\n                    value = {this.state.amount}\n                    onChange = {this.updateAmount}\n                    />\n                </FormGroup>\n                <div>\n                    <Button \n                    bsStyle=\"info\"\n                    onClick={this.conductTransaction}\n                    >Submit</Button>\n                </div>\n            </div>\n        )\n    }\n}\n\nexport default ConductTransaction;"},"lineCount":null}},"error":null,"hash":"6690a35586c834ddc6d35c2c958aed7f","cacheData":{"env":{}}}