                 {\n                        [...Array(Math.ceil(this.state.blocksLength/5)).keys()].map(key => {\n                            const paginatedId = key+1;\n\n                            return (\n                                <span key = {key} onClick={this.fetchPaginatedBlocks(paginatedId)}>\n                                    <Button bsStyle=\"info\" bsSize = \"small\">\n                                        {paginatedId}\n                                    </Button>{' '}\n                                </span>\n                            )\n                        })\n                    }\n                </div>\n                {\n                    this.state.blocks.map(block => {\n                        return (\n                            <Block key = {block.hash} block = {block} />\n                        )\n                    })\n                }\n            </div>\n        );\n    }\n}\n\nexport default Blocks;"},"lineCount":null}},"error":null,"hash":"a57f4e3d0bec0bf5bc73531a89116dfc","cacheData":{"env":{}}}