,{"generated":{"line":106,"column":171},"source":"components/Block.js","original":{"line":61,"column":17}},{"name":"displayTransaction","generated":{"line":106,"column":176},"source":"components/Block.js","original":{"line":61,"column":22}},{"generated":{"line":106,"column":194},"source":"components/Block.js","original":{"line":58,"column":12}},{"generated":{"line":106,"column":195},"source":"components/Block.js","original":{"line":57,"column":8}},{"generated":{"line":107,"column":0},"source":"components/Block.js","original":{"line":64,"column":5}},{"name":"Component","generated":{"line":111,"column":2},"source":"components/Block.js","original":{"line":5,"column":20}},{"generated":{"line":113,"column":0},"source":"components/Block.js","original":{"line":65,"column":1}},{"name":"Block","generated":{"line":114,"column":15},"source":"components/Block.js","original":{"line":67,"column":15}}],"sources":{"components/Block.js":"import React, { Component } from 'react';\nimport  { Button } from 'react-bootstrap';\nimport Transaction from './Transaction';\n\nclass Block extends Component {\n    state = { displayTransaction: false };\n\n    toggleTransaction = () => {\n        this.setState({ displayTransaction: !this.state.displayTransaction });\n    }\n\n    get displayTransaction() {\n        const { data } = this.props.block;\n        const stringifiedData = JSON.stringify(data);\n\n        const dataDisplay = stringifiedData.length > 35 ? `${stringifiedData.substring(0, 35)}...`\n        : stringifiedData;\n\n        if(this.state.displayTransaction) {\n            return (\n                <div>\n                    {\n                        data.map(transaction => (\n                            <div key={transaction.id}>\n                                <hr/>\n                                <Transaction transaction = {transaction} />\n                            </div>\n                        ))\n                    }\n                    <br/>\n                    <Button \n                    bsStyle=\"info\" \n                    bsSize = \"small\" \n                    onClick = {this.toggleTransaction}>Hide Info</Button>\n                </div>\n            )\n        }\n\n        return (\n            <div>\n            <div>Data: {dataDisplay}</div>\n            <Button \n            bsStyle=\"info\" \n            bsSize = \"small\" \n            onClick = {this.toggleTransaction}>Show Info</Button>\n            </div>\n        )\n    }\n\n    render() {\n\n        const { timestamp, hash, data } = this.props.block;\n\n        const hashDisplay = `${hash.substring(0, 15)}...`;\n   \n\n        return (\n            <div className = 'Block'>\n                <div>Hash: {hashDisplay}</div>\n                <div>Timestamp: {new Date(timestamp).toLocaleString()}</div>\n                {this.displayTransaction}\n            </div>\n        )\n    }\n};\n\nexport default Block;"},"lineCount":null}},"error":null,"hash":"3a220d75f49bbbb3912addb7990d1ecd","cacheData":{"env":{}}}